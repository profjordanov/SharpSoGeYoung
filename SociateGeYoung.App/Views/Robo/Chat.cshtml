<!DOCTYPE html>
<html lang="en">

<head>

    <!-- Meta Tag -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- SEO -->
    <meta name="description" content="150 words">
    <meta name="author" content="uipasta">
    <meta name="url" content="http://www.yourdomainname.com">
    <meta name="copyright" content="company name">
    <meta name="robots" content="index,follow">


    <title>@ViewBag.Title</title>

    <!-- All CSS Plugins -->
    <link rel="stylesheet" type="text/css" href="~/RoboTemplate/css/plugin.css">
    <!-- Main CSS Stylesheet -->
    <link rel="stylesheet" type="text/css" href="~/RoboTemplate/css/style.css">

    <!-- Google Web Fonts  -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:400,300,500,600,700">


    <!-- HTML5 shiv and Respond.js support IE8 or Older for HTML5 elements and media queries -->
    <!--[if lt IE 9]>
       <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
       <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->


</head>

<body>



    <!-- Preloader Start -->
    <div id="preloader">
        <div class="loader"></div>
    </div>
    <!-- Preloader End -->
    <!-- Home & Menu Section Start -->
    <header id="home" class="home-section">

        <div class="header-top-area">
            <div class="container">
                <div class="row">

                    <div class="col-sm-3">
                        <div class="logo">
                            <a href="#">SogeRobo</a>
                        </div>
                    </div>

                    <div class="col-sm-9">
                        <div class="navigation-menu">
                            <div class="navbar">
                                <div class="navbar-header">
                                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                        <span class="sr-only">Toggle navigation</span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                    </button>
                                </div>
                                <div class="navbar-collapse collapse">
                                    <ul class="nav navbar-nav navbar-right">
                                        <li class="active">
                                            @Html.ActionLink("#SoGeYoung", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                                        </li>
                                        <li>
                                            @Html.ActionLink("Тест 1", "Index", "FirstTest")
                                        </li>
                                        <li>
                                            @Html.ActionLink("Обяви", "All", "Ads")
                                        </li>
                                        <li>
                                            @Html.ActionLink("Контакти", "Contact", "Home")
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="home-section-background" data-stellar-background-ratio="0.6">
            <div class="display-table">
                <div class="display-table-cell">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-12 text-center">
                                <iframe src='https://webchat.botframework.com/embed/minerobo?s=eHGV9zwu36M.cwA.Zgk.m4uul5QMdUbGTWOTO5qZa6DKaPXmglrLZnxHC3Qil0M'
                                        height="400" width="400" style="background: white"></iframe>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </header>
    <footer class="footer-section">
        <div class="container">
            <div class="row">

                <div class="col-md-4 text-left">
                    <p>© Copyright 2018 </p>
                </div>



            </div>
        </div>
    </footer>
    <!-- Footer End -->
    <!-- Back to Top Start -->
    <a href="#" class="scroll-to-top"><i class="fa fa-angle-up"></i></a>
    <!-- Back to Top End -->
    <!-- All Javascript Plugins  -->
    <script type="text/javascript" src="~/RoboTemplate/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/RoboTemplate/js/plugin.js"></script>
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?key=AIzaSyC0HAKwKinpoFKNGUwRBgkrKhF-sIqFUNA"></script>

    <!-- Main Javascript File  -->
    <script type="text/javascript" src="~/RoboTemplate/js/scripts.js"></script>


</body>
</html>
